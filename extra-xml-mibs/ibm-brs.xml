<?xml version="1.0" encoding="UTF-8"?>
<mib definition="BRS-MIB">
    <imports>
        <mibImport>
            <sourceMib>RFC1155-SMI</sourceMib>
            <import>enterprises</import>
            <import>Counter</import>
            <import>IpAddress</import>
        </mibImport>
        <mibImport>
            <sourceMib>RFC-1212</sourceMib>
            <import>OBJECT-TYPE</import>
        </mibImport>
        <mibImport>
            <sourceMib>RFC1213-MIB</sourceMib>
            <import>ifIndex</import>
            <import>DisplayString</import>
        </mibImport>
    </imports>
    <objects>
        <mibObject>
            <objectIdentifier>
                <objectName>proteon</objectName>
                <objectId>1</objectId>
                <parent>enterprises</parent>
            </objectIdentifier>
        </mibObject>
        <mibObject>
            <objectIdentifier>
                <objectName>proAdmin</objectName>
                <objectId>1</objectId>
                <parent>proteon</parent>
            </objectIdentifier>
        </mibObject>
        <mibObject>
            <objectIdentifier>
                <objectName>proFeature</objectName>
                <objectId>7</objectId>
                <parent>proAdmin</parent>
            </objectIdentifier>
        </mibObject>
        <mibObject>
            <objectIdentifier>
                <objectName>proFeatureBrs</objectName>
                <objectId>1</objectId>
                <parent>proFeature</parent>
            </objectIdentifier>
        </mibObject>
        <mibObject>
            <objectIdentifier>
                <objectName>proBrsMib</objectName>
                <objectId>1</objectId>
                <parent>proFeatureBrs</parent>
            </objectIdentifier>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>proBrsNumber</objectName>
                <objectId>1</objectId>
                <parent>proBrsMib</parent>
                <syntax>
                    <type>INTEGER</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>The number of interfaces enabled for BRS.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>proBrsPortTable</objectName>
                <objectId>2</objectId>
                <parent>proBrsMib</parent>
                <syntax>
                    <sequence>ProBrsPortEntry</sequence>
                </syntax>
                <access>not-accessible</access>
                <status>mandatory</status>
                <description>A list of BRS ports/interfaces.  The number of entries is&lt;br&gt;given by the value of proBrsNumber.  Only ports/interfaces&lt;br&gt;that have BRS enabled will appear in this table.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>proBrsPortEntry</objectName>
                <objectId>1</objectId>
                <parent>proBrsPortTable</parent>
                <syntax>
                    <type>ProBrsPortEntry</type>
                </syntax>
                <access>not-accessible</access>
                <status>mandatory</status>
                <description>Status and parameter values for an interface.</description>
                <indices>
                    <index>ifIndex</index>
                </indices>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>proBrsClassNumber</objectName>
                <objectId>1</objectId>
                <parent>proBrsPortEntry</parent>
                <syntax>
                    <type>INTEGER</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>The number of classes on this interface.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>proBrsDefaultClassName</objectName>
                <objectId>2</objectId>
                <parent>proBrsPortEntry</parent>
                <syntax>
                    <type>DisplayString(SIZE(0..10))</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>The name of the default class.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>proBrsDefaultPriority</objectName>
                <objectId>3</objectId>
                <parent>proBrsPortEntry</parent>
                <syntax>
                    <type>BRSPriority</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>The default priority for the default class.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>proBrsClassTable</objectName>
                <objectId>3</objectId>
                <parent>proBrsMib</parent>
                <syntax>
                    <sequence>ProBrsClassEntry</sequence>
                </syntax>
                <access>not-accessible</access>
                <status>mandatory</status>
                <description>A list of BRS Classes per interface.  The number of&lt;br&gt;entries is given by the value of proBrsClassNumber in&lt;br&gt;the previous proBrsPortTable group.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>proBrsClassEntry</objectName>
                <objectId>1</objectId>
                <parent>proBrsClassTable</parent>
                <syntax>
                    <type>ProBrsClassEntry</type>
                </syntax>
                <access>not-accessible</access>
                <status>mandatory</status>
                <description>Status and parameter values for a class.</description>
                <indices>
                    <index>ifIndex</index>
                    <index>proBrsClassId</index>
                </indices>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>proBrsClassId</objectName>
                <objectId>1</objectId>
                <parent>proBrsClassEntry</parent>
                <syntax>
                    <type>INTEGER</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>A unique ID for each class.  Its value ranges&lt;br&gt;between 1 and the value of proBrsClassNumber in the&lt;br&gt;proBrsPortTable.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>proBrsClassName</objectName>
                <objectId>2</objectId>
                <parent>proBrsClassEntry</parent>
                <syntax>
                    <type>DisplayString(SIZE(0..10))</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>The class name.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>proBrsClassAlloc</objectName>
                <objectId>3</objectId>
                <parent>proBrsClassEntry</parent>
                <syntax>
                    <type>INTEGER</type>
                </syntax>
                <access>read-write</access>
                <status>mandatory</status>
                <description>Bandwidth Allocation percentage.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>proBrsClassBytes</objectName>
                <objectId>4</objectId>
                <parent>proBrsClassEntry</parent>
                <syntax>
                    <type>INTEGER</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>The number of bytes transmitted in this class.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>proBrsClassPkts</objectName>
                <objectId>5</objectId>
                <parent>proBrsClassEntry</parent>
                <syntax>
                    <type>INTEGER</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>The number of packets transmitted in this class.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>proBrsClassDiscs</objectName>
                <objectId>6</objectId>
                <parent>proBrsClassEntry</parent>
                <syntax>
                    <type>INTEGER</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>The number of bytes discarded in this class.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>proBrsFilterTable</objectName>
                <objectId>4</objectId>
                <parent>proBrsMib</parent>
                <syntax>
                    <sequence>ProBrsFilterEntry</sequence>
                </syntax>
                <access>not-accessible</access>
                <status>mandatory</status>
                <description>A list of BRS Protocols/Filters.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>proBrsFilterEntry</objectName>
                <objectId>1</objectId>
                <parent>proBrsFilterTable</parent>
                <syntax>
                    <type>ProBrsFilterEntry</type>
                </syntax>
                <access>not-accessible</access>
                <status>mandatory</status>
                <description>Status and parameter values for a filter.</description>
                <indices>
                    <index>ifIndex</index>
                    <index>proBrsClassId</index>
                    <index>proBrsFilterId</index>
                </indices>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>proBrsFilterId</objectName>
                <objectId>1</objectId>
                <parent>proBrsFilterEntry</parent>
                <syntax>
                    <type>INTEGER</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>A unique value for each filter.  Its value is the&lt;br&gt;protocol number or a filter number + the maximum&lt;br&gt;protocol number.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>proBrsFilterName</objectName>
                <objectId>2</objectId>
                <parent>proBrsFilterEntry</parent>
                <syntax>
                    <type>DisplayString(SIZE(0..10))</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>The filter name.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>proBrsFilterPriority</objectName>
                <objectId>3</objectId>
                <parent>proBrsFilterEntry</parent>
                <syntax>
                    <type>INTEGER</type>
                    <valueList>
                        <valueItem>
                            <label>low</label>
                            <val>1</val>
                        </valueItem>
                        <valueItem>
                            <label>normal</label>
                            <val>2</val>
                        </valueItem>
                        <valueItem>
                            <label>high</label>
                            <val>3</val>
                        </valueItem>
                        <valueItem>
                            <label>urgent</label>
                            <val>4</val>
                        </valueItem>
                    </valueList>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>The priority of the filter.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>proBrsFilterPortType</objectName>
                <objectId>4</objectId>
                <parent>proBrsFilterEntry</parent>
                <syntax>
                    <type>INTEGER</type>
                    <valueList>
                        <valueItem>
                            <label>tcp</label>
                            <val>1</val>
                        </valueItem>
                        <valueItem>
                            <label>udp</label>
                            <val>2</val>
                        </valueItem>
                        <valueItem>
                            <label>other</label>
                            <val>3</val>
                        </valueItem>
                    </valueList>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>This field is relevent to the UDP_TCP port filters only&lt;br&gt;(where proBRSFilterId is UDP_TCP1, UDP_TCP2, UDP_TCP3,&lt;br&gt;UDP_TCP4 or UDP_TCP5).&lt;br&gt;It signifies whether UDP/IP or TCP/IP traffic should be&lt;br&gt;filtered. Valid values are tcp(1), udp(2) or other(3),&lt;br&gt;which is specified for other protocol/filter types.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>proBrsFilterLowPortNum</objectName>
                <objectId>5</objectId>
                <parent>proBrsFilterEntry</parent>
                <syntax>
                    <type>INTEGER (0..65535)</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>This field is relevent to the UDP_TCP port filters only&lt;br&gt;(where proBRSFilterId is UDP_TCP1, UDP_TCP2, UDP_TCP3,&lt;br&gt;UDP_TCP4 or UDP_TCP5).&lt;br&gt;It signifies the low end of a range of port numbers on&lt;br&gt;which a UDP_TCP filter is based. Valid values for&lt;br&gt;UDP_TCP filters are from 1 to 65535.&lt;br&gt;&lt;br&gt;This field will be zero for other protocol/filter types.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>proBrsFilterHighPortNum</objectName>
                <objectId>6</objectId>
                <parent>proBrsFilterEntry</parent>
                <syntax>
                    <type>INTEGER (0..65535)</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>This is field relevent to the UDP_TCP port filters only&lt;br&gt;(where proBRSFilterId is UDP_TCP1, UDP_TCP2, UDP_TCP3,&lt;br&gt;UDP_TCP4 or UDP_TCP5).&lt;br&gt;It signifies the high end of a range of port numbers. It&lt;br&gt;can range from 1 to 65535 and is equal to or greater&lt;br&gt;than the low port range.&lt;br&gt;&lt;br&gt;This field will be 0 for other protocol/filter types.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>proBrsFilterIpAddr</objectName>
                <objectId>7</objectId>
                <parent>proBrsFilterEntry</parent>
                <syntax>
                    <type>IpAddress</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>This field is relevent to the UDP_TCP port filters only.&lt;br&gt;If the IP address is non zero, the IP address and&lt;br&gt;the port number range (defined by the Low Port and High&lt;br&gt;Port) are used together to define a socket in the&lt;br&gt;filtering  criteria for the UDP_TCP filter.&lt;br&gt;If the IP address is zero, only the port range criteria&lt;br&gt;criteria is used for the UDP_TCP filter.&lt;br&gt;&lt;br&gt;This field will be zero for other protocol/filter types.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>proBrsFilterTag</objectName>
                <objectId>8</objectId>
                <parent>proBrsFilterEntry</parent>
                <syntax>
                    <type>INTEGER (0..64)</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>This field is relevent only for BRS tags  (where&lt;br&gt;proBRSFilterId is TAG1, TAG2, TAG3, TAG4 or TAG5).&lt;br&gt;&lt;br&gt;The Tag Value identifies a set of filter items&lt;br&gt;associated with a specific tag value via MAC&lt;br&gt;Filtering configuration. The association of a&lt;br&gt;Tag Value with one of the 5 BRS tags allows the set&lt;br&gt;of filtered items to be assigned to a class and&lt;br&gt;priority configured for the BRS tag.&lt;br&gt;&lt;br&gt;Valid values are 1 to 64.&lt;br&gt;&lt;br&gt;This field will be 0 for other protocol/filter types.</description>
            </objectType>
        </mibObject>
    </objects>
</mib>
