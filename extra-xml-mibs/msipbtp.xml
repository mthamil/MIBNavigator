                <objectId>3</objectId>
                <parent>ifConfigEntry</parent>
                <syntax>
                    <type>INTEGER</type>
                    <nameValuePairs>
                        <nameValuePair>
                            <pairName>disabled</pairName>
                            <pairValue>1</pairValue>
                        </nameValuePair>
                        <nameValuePair>
                            <pairName>enabled</pairName>
                            <pairValue>2</pairValue>
