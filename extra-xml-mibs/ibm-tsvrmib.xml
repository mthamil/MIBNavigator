<?xml version="1.0" encoding="UTF-8"?>
<mib definition="THIN-SERVER-MIB">
    <imports>
        <mibImport>
            <sourceMib>SNMPv2-SMI-v1</sourceMib>
            <import>Counter32</import>
            <import>Integer32</import>
            <import>Gauge32</import>
            <import>IpAddress</import>
        </mibImport>
        <mibImport>
            <sourceMib>RFC-1212</sourceMib>
            <import>OBJECT-TYPE</import>
        </mibImport>
        <mibImport>
            <sourceMib>RFC1155-SMI</sourceMib>
            <import>enterprises</import>
        </mibImport>
        <mibImport>
            <sourceMib>SNMPv2-TC-v1</sourceMib>
            <import>TruthValue</import>
            <import>DisplayString</import>
        </mibImport>
    </imports>
    <objects>
        <mibObject>
            <objectIdentifier>
                <objectName>ibmThinServer</objectName>
                <objectId>3</objectId>
                <parent>ibmIROCsystem</parent>
            </objectIdentifier>
        </mibObject>
        <mibObject>
            <objectIdentifier>
                <objectName>genInfo</objectName>
                <objectId>1</objectId>
                <parent>ibmThinServer</parent>
            </objectIdentifier>
        </mibObject>
        <mibObject>
            <objectIdentifier>
                <objectName>genStat</objectName>
                <objectId>2</objectId>
                <parent>ibmThinServer</parent>
            </objectIdentifier>
        </mibObject>
        <mibObject>
            <objectIdentifier>
                <objectName>msConnStats</objectName>
                <objectId>3</objectId>
                <parent>ibmThinServer</parent>
            </objectIdentifier>
        </mibObject>
        <mibObject>
            <objectIdentifier>
                <objectName>rfsStat</objectName>
                <objectId>4</objectId>
                <parent>ibmThinServer</parent>
            </objectIdentifier>
        </mibObject>
        <mibObject>
            <objectIdentifier>
                <objectName>tftpStat</objectName>
                <objectId>5</objectId>
                <parent>ibmThinServer</parent>
            </objectIdentifier>
        </mibObject>
        <mibObject>
            <objectIdentifier>
                <objectName>nfsStat</objectName>
                <objectId>6</objectId>
                <parent>ibmThinServer</parent>
            </objectIdentifier>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>thinserverEnable</objectName>
                <objectId>1</objectId>
                <parent>genInfo</parent>
                <syntax>
                    <type>INTEGER</type>
                    <valueList>
                        <valueItem>
                            <label>disabled</label>
                            <val>0</val>
                        </valueItem>
                        <valueItem>
                            <label>enabled</label>
                            <val>1</val>
                        </valueItem>
                        <valueItem>
                            <label>passthru</label>
                            <val>2</val>
                        </valueItem>
                        <valueItem>
                            <label>disabledpending</label>
                            <val>3</val>
                        </valueItem>
                    </valueList>
                </syntax>
                <access>read-write</access>
                <status>mandatory</status>
                <description>This device provides thin server support.&lt;br&gt;If enabled(1) then this device will provide&lt;br&gt;local file caching of selected files. If&lt;br&gt;passthru(2) then this device will have connections&lt;br&gt;to the clients and pass any frames received on those&lt;br&gt;connections to the master server and pass frames&lt;br&gt;received from the master server to the appropriate&lt;br&gt;client. If disabled(0) then the clients will contact&lt;br&gt;the master server directly.  If disabledpending(3),&lt;br&gt;then thin server support is in the process of disabling.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>thinserverRefreshInterval</objectName>
                <objectId>2</objectId>
                <parent>genInfo</parent>
                <syntax>
                    <type>Integer32</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>This is the number of days between refreshes&lt;br&gt;of this devices cache.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>thinserverRefreshTime</objectName>
                <objectId>3</objectId>
                <parent>genInfo</parent>
                <syntax>
                    <type>DisplayString</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>This is the time of day (12:01:00)&lt;br&gt;at which this device will verify that it has&lt;br&gt;the same version of the files as the master server,&lt;br&gt;and will download any files which are not the&lt;br&gt;same version.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>thinserverRefreshNow</objectName>
                <objectId>4</objectId>
                <parent>genInfo</parent>
                <syntax>
                    <type>INTEGER</type>
                    <valueList>
                        <valueItem>
                            <label>noaction</label>
                            <val>1</val>
                        </valueItem>
                        <valueItem>
                            <label>refreshnow</label>
                            <val>2</val>
                        </valueItem>
                    </valueList>
                </syntax>
                <access>read-write</access>
                <status>mandatory</status>
                <description>This flag indicates that the device&lt;br&gt;should immediately refresh all files which&lt;br&gt;it is caching.  A read of this object will&lt;br&gt;always return noaction(1).</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>thinserverMemory</objectName>
                <objectId>5</objectId>
                <parent>genInfo</parent>
                <syntax>
                    <type>Integer32</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>This is the amount of memory in kilobytes&lt;br&gt;calculated at runtime to be reserved for caching&lt;br&gt;files.  This may be the same amount as the configured&lt;br&gt;memory.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>thinserverHardFileSpace</objectName>
                <objectId>6</objectId>
                <parent>genInfo</parent>
                <syntax>
                    <type>Integer32</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>This is the total amount of hard file space,&lt;br&gt;in kilobytes, which can be used for the files&lt;br&gt;being cached for the thin server function.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>thinserverHardFileUsed</objectName>
                <objectId>7</objectId>
                <parent>genInfo</parent>
                <syntax>
                    <type>Gauge32</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>This is the total amount of hard file space,&lt;br&gt;in kilobytes, which is currently being used&lt;br&gt;for the files related to the thin server function.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>thinserverNumFilesCached</objectName>
                <objectId>8</objectId>
                <parent>genInfo</parent>
                <syntax>
                    <type>Gauge32</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>This is the total number of files being&lt;br&gt;cached by the thin server function.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>thinserverMasterServerIPAddress</objectName>
                <objectId>9</objectId>
                <parent>genInfo</parent>
                <syntax>
                    <type>IpAddress</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>This is the IP address of the server for&lt;br&gt;which this device is staging files.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>thinserverSyncProtocol</objectName>
                <objectId>10</objectId>
                <parent>genInfo</parent>
                <syntax>
                    <type>INTEGER</type>
                    <valueList>
                        <valueItem>
                            <label>rfs400</label>
                            <val>1</val>
                        </valueItem>
                        <valueItem>
                            <label>nfs</label>
                            <val>2</val>
                        </valueItem>
                    </valueList>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>This is the protocol which will be used&lt;br&gt;to determine whether or not the cache needs&lt;br&gt;to be updated and if so, this protocol will&lt;br&gt;be used to retrieve the new version of the&lt;br&gt;file(s).</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>thinserverPreloadListName</objectName>
                <objectId>11</objectId>
                <parent>genInfo</parent>
                <syntax>
                    <type>DisplayString(SIZE(1..255))</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>This is the fully-qualified name of the&lt;br&gt;file which contains the pre-load list.&lt;br&gt;This name can be 1024 bytes long but only&lt;br&gt;the last 255 bytes will be displayed.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>thinserverMountPointsTable</objectName>
                <objectId>12</objectId>
                <parent>genInfo</parent>
                <syntax>
                    <sequence>ThinserverMountPointsEntry</sequence>
                </syntax>
                <access>not-accessible</access>
                <status>mandatory</status>
                <description>When NFS is used as the synchronization protocol,&lt;br&gt;then this table lists the directories to be included&lt;br&gt;in the mount point from which files will be cached.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>thinserverMountPointsEntry</objectName>
                <objectId>1</objectId>
                <parent>thinserverMountPointsTable</parent>
                <syntax>
                    <type>ThinserverMountPointsEntry</type>
                </syntax>
                <access>not-accessible</access>
                <status>mandatory</status>
                <description>An entry containing objects&lt;br&gt;that define which directories are to&lt;br&gt;be included for the mount point.</description>
                <indices>
                    <index>thinserverMountPointsIndex</index>
                </indices>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>thinserverMountPointsIndex</objectName>
                <objectId>1</objectId>
                <parent>thinserverMountPointsEntry</parent>
                <syntax>
                    <type>Integer32</type>
                </syntax>
                <access>not-accessible</access>
                <status>mandatory</status>
                <description>A unique value for each mount point.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>thinserverMountPointsDescr</objectName>
                <objectId>2</objectId>
                <parent>thinserverMountPointsEntry</parent>
                <syntax>
                    <type>DisplayString(SIZE(1..255))</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>A textual string containing the name&lt;br&gt;of a mount point (NFS).  This name can&lt;br&gt;be 1024 bytes long, but only the last&lt;br&gt;255 bytes will be displayed.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>thinserverMountPointsScope</objectName>
                <objectId>3</objectId>
                <parent>thinserverMountPointsEntry</parent>
                <syntax>
                    <type>INTEGER</type>
                    <valueList>
                        <valueItem>
                            <label>include</label>
                            <val>1</val>
                        </valueItem>
                        <valueItem>
                            <label>exclude</label>
                            <val>2</val>
                        </valueItem>
                    </valueList>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>If include(1), then that directory is&lt;br&gt;included in the mount point.  If exclude(2),&lt;br&gt;then that directory is excluded from the&lt;br&gt;mount point.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>thinserverPacketTimeout</objectName>
                <objectId>13</objectId>
                <parent>genInfo</parent>
                <syntax>
                    <type>Integer32</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>This is the number of seconds defined by the&lt;br&gt;user until a packet will time out.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>thinserverMaxRetries</objectName>
                <objectId>14</objectId>
                <parent>genInfo</parent>
                <syntax>
                    <type>Integer32</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>This is the maximum number of times defined by the&lt;br&gt;user that the TFTP daemon will try to resend a packet.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>thinserverMaxSegSize</objectName>
                <objectId>15</objectId>
                <parent>genInfo</parent>
                <syntax>
                    <type>Integer32</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>This is the maximum segment block size defined by the&lt;br&gt;user.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>thinserverUseHardFile</objectName>
                <objectId>16</objectId>
                <parent>genInfo</parent>
                <syntax>
                    <type>TruthValue</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>This tells whether or not we are to use the hardfile&lt;br&gt;when caching.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>thinserverRestartNow</objectName>
                <objectId>17</objectId>
                <parent>genInfo</parent>
                <syntax>
                    <type>INTEGER</type>
                    <valueList>
                        <valueItem>
                            <label>noaction</label>
                            <val>1</val>
                        </valueItem>
                        <valueItem>
                            <label>restartnow</label>
                            <val>2</val>
                        </valueItem>
                    </valueList>
                </syntax>
                <access>read-write</access>
                <status>mandatory</status>
                <description>This flag indicates that the device&lt;br&gt;should immediately restart the thinserver&lt;br&gt;feature.  A read of this object will&lt;br&gt;always return noaction(1).</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>thinserverConfiguredMemory</objectName>
                <objectId>18</objectId>
                <parent>genInfo</parent>
                <syntax>
                    <type>Integer32</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>This is the amount of memory in kilobytes defined&lt;br&gt;by the user to be reserved for caching files.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>thinserverFilesOpenCurrently</objectName>
                <objectId>1</objectId>
                <parent>genStat</parent>
                <syntax>
                    <type>Gauge32</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>This is the number of files currently&lt;br&gt;in the open state because of the thin server&lt;br&gt;function.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>thinserverTotalFileOpens</objectName>
                <objectId>2</objectId>
                <parent>genStat</parent>
                <syntax>
                    <type>Counter32</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>This is the total number of files that have been&lt;br&gt;opened since the last reset of the genStat counters&lt;br&gt;because of the thin server function.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>thinserverReadMissesDirty</objectName>
                <objectId>3</objectId>
                <parent>genStat</parent>
                <syntax>
                    <type>Counter32</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>Number of times a read was relayed to the&lt;br&gt;server because the file on this device&lt;br&gt;is different than the version on the&lt;br&gt;master server.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>thinserverReadMissesNotPresent</objectName>
                <objectId>4</objectId>
                <parent>genStat</parent>
                <syntax>
                    <type>Counter32</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>Number of times a read was relayed to the&lt;br&gt;server because this device does not have&lt;br&gt;the desired file.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>thinserverOpenFailsFileLocked</objectName>
                <objectId>5</objectId>
                <parent>genStat</parent>
                <syntax>
                    <type>Counter32</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>Number of times a read was relayed to the&lt;br&gt;server because this device cannot serve the&lt;br&gt;desired file because it is locked.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>thinserverNoRoomOnHardFile</objectName>
                <objectId>6</objectId>
                <parent>genStat</parent>
                <syntax>
                    <type>Counter32</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>Number of times the thin server function&lt;br&gt;attempted to cache a file but could not&lt;br&gt;because there was no more thin server space&lt;br&gt;available on the hard disk.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>thinserverResetGenCounters</objectName>
                <objectId>7</objectId>
                <parent>genStat</parent>
                <syntax>
                    <type>INTEGER</type>
                    <valueList>
                        <valueItem>
                            <label>noaction</label>
                            <val>1</val>
                        </valueItem>
                        <valueItem>
                            <label>resetnow</label>
                            <val>2</val>
                        </valueItem>
                    </valueList>
                </syntax>
                <access>read-write</access>
                <status>mandatory</status>
                <description>Reset the general counters to zero.  A read of&lt;br&gt;this object will always return noaction(1).</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>thinserverNumRefreshes</objectName>
                <objectId>1</objectId>
                <parent>msConnStats</parent>
                <syntax>
                    <type>Counter32</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>Number of times the cache has been successfully&lt;br&gt;refreshed.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>thinserverNumRefreshFail</objectName>
                <objectId>2</objectId>
                <parent>msConnStats</parent>
                <syntax>
                    <type>Counter32</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>Number of times the cache has failed to refresh&lt;br&gt;because the connection to the master server&lt;br&gt;could not be established.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>thinserverNumFilesRefreshed</objectName>
                <objectId>3</objectId>
                <parent>msConnStats</parent>
                <syntax>
                    <type>Counter32</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>Number of files which have been&lt;br&gt;refreshed in the cache.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>thinserverLastFileUpdate</objectName>
                <objectId>4</objectId>
                <parent>msConnStats</parent>
                <syntax>
                    <type>DisplayString</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>This is the date and time of day (02/03/1998 23:08:09)&lt;br&gt;that the most recent file refresh occurred.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>thinserverResetMSConnStats</objectName>
                <objectId>5</objectId>
                <parent>msConnStats</parent>
                <syntax>
                    <type>INTEGER</type>
                    <valueList>
                        <valueItem>
                            <label>noaction</label>
                            <val>1</val>
                        </valueItem>
                        <valueItem>
                            <label>resetnow</label>
                            <val>2</val>
                        </valueItem>
                    </valueList>
                </syntax>
                <access>read-write</access>
                <status>mandatory</status>
                <description>Reset the MS connection counters to zero.  A&lt;br&gt;read of this object will always return noaction(1).</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>thinserverRFSTotalClients</objectName>
                <objectId>1</objectId>
                <parent>rfsStat</parent>
                <syntax>
                    <type>Counter32</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>The number of clients which have connected&lt;br&gt;to this device using RFS/400 protocol.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>thinserverRFSCurrentClients</objectName>
                <objectId>2</objectId>
                <parent>rfsStat</parent>
                <syntax>
                    <type>Gauge32</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>The number of clients currently connected&lt;br&gt;to this device using RFS/400 protocol.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>thinserverRFSFilesServed</objectName>
                <objectId>3</objectId>
                <parent>rfsStat</parent>
                <syntax>
                    <type>Counter32</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>This is the number of open requests for which&lt;br&gt;this device opened a cached file.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>thinserverRFSFilesServedByMS</objectName>
                <objectId>4</objectId>
                <parent>rfsStat</parent>
                <syntax>
                    <type>Counter32</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>This is the number of open requests for which&lt;br&gt;this device has passed the request thru to the&lt;br&gt;master server because the local copy either&lt;br&gt;does not exist, is out-of-date, or is not&lt;br&gt;accessable.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>thinserverNum449Accepts</objectName>
                <objectId>5</objectId>
                <parent>rfsStat</parent>
                <syntax>
                    <type>Counter32</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>The number of port 449 (port mapper) accepts.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>thinserverNum449ConnsActive</objectName>
                <objectId>6</objectId>
                <parent>rfsStat</parent>
                <syntax>
                    <type>Gauge32</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>The number of port 449 (port mapper) connections&lt;br&gt;currently active.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>thinserverNum8473Accepts</objectName>
                <objectId>7</objectId>
                <parent>rfsStat</parent>
                <syntax>
                    <type>Counter32</type>
