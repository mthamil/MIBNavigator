        <object>
            <objectType>
                <objectName>ibmnetuPCIAdapOperStatus</objectName>
                <objectId>3</objectId>
                <parent>ibmnetuPCIAdapEntry</parent>
                <syntax>
                    <type>INTEGER</type>
                    <nameValuePairs>
                        <nameValuePair>
                            <pairName>unknown</pairName>
                            <pairValue>1</pairValue>
                        </nameValuePair>
                        <nameValuePair>
                            <pairName>not-configured</pairName>
                            <pairValue>2</pairValue>
                        </nameValuePair>
                        <nameValuePair>
                            <pairName>not-present</pairName>
                            <pairValue>3</pairValue>
                        </nameValuePair>
                        <nameValuePair>
                            <pairName>does-not-apply</pairName>
                            <pairValue>4</pairValue>
                        </nameValuePair>
                        <nameValuePair>
                            <pairName>enable-pending</pairName>
                            <pairValue>5</pairValue>
                        </nameValuePair>
                        <nameValuePair>
                            <pairName>enabled</pairName>
