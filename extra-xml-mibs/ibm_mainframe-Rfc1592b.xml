<?xml version="1.0" encoding="UTF-8"?>
<mib definition="DPI20-MIB">
    <imports>
        <mibImport>
            <sourceMib>SNMPv2-SMI</sourceMib>
            <import>MODULE-IDENTITY</import>
            <import>OBJECT-TYPE</import>
            <import>enterprises</import>
        </mibImport>
        <mibImport>
            <sourceMib>SNMPv2-TC</sourceMib>
            <import>DisplayString</import>
        </mibImport>
        <mibImport>
            <sourceMib>SNMPv2-CONF</sourceMib>
            <import>MODULE-COMPLIANCE</import>
            <import>OBJECT-GROUP</import>
        </mibImport>
    </imports>
    <objects>
        <mibObject>
            <objectIdentifier>
                <objectName>ibm</objectName>
                <objectId>2</objectId>
                <parent>enterprises</parent>
            </objectIdentifier>
        </mibObject>
        <mibObject>
            <objectIdentifier>
                <objectName>ibmDPI</objectName>
                <objectId>2</objectId>
                <parent>ibm</parent>
            </objectIdentifier>
        </mibObject>
        <mibObject>
            <moduleIdentity>
                <objectName>dpi20MIB</objectName>
                <objectId>1</objectId>
                <parent>ibmDPI</parent>
                <lastUpdated>9609300000Z</lastUpdated>
                <organization>IBM Research - T.J. Watson Research Center</organization>
                <contactInfo>Bert Wijnen&lt;br&gt;&lt;br&gt;Postal:    IBM International Operations&lt;br&gt;Watsonweg 2&lt;br&gt;1423 ND Uithoorn&lt;br&gt;The Netherlands&lt;br&gt;&lt;br&gt;Tel:       +31 79-322-8316&lt;br&gt;&lt;br&gt;E-mail:    wijnen@vnet.ibm.com&lt;br&gt;(IBM internal: wijnen at uitvm1)</contactInfo>
                <description>The MIB module describing DPI objects.</description>
                <revision revId="9609300000Z">
                    <description>Changes in this revision&lt;br&gt;- Added dpiPathNameForUnixStream</description>
                </revision>
                <revision revId="9605010000Z">
                    <description>Changes in this revision&lt;br&gt;- Change telephone number</description>
                </revision>
                <revision revId="9505260000Z">
                    <description>Changes in this revision&lt;br&gt;- Enable SNMPv2 items&lt;br&gt;- import from SNMPv2 documents&lt;br&gt;- Add conformance statements</description>
                </revision>
            </moduleIdentity>
        </mibObject>
        <mibObject>
            <objectIdentifier>
                <objectName>dpiPort</objectName>
                <objectId>1</objectId>
                <parent>dpi20MIB</parent>
            </objectIdentifier>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>dpiPortForTCP</objectName>
                <objectId>1</objectId>
                <parent>dpiPort</parent>
                <syntax>
                    <type>INTEGER (0..65535)</type>
                </syntax>
                <access>read-only</access>
                <status>current</status>
                <description>The TCP port number on which the agent listens for&lt;br&gt;DPI connections.&lt;br&gt;If the value is zero, it means that the agent is&lt;br&gt;not listening for TCP DPI connections.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>dpiPortForUDP</objectName>
                <objectId>2</objectId>
                <parent>dpiPort</parent>
                <syntax>
                    <type>INTEGER (0..65535)</type>
                </syntax>
                <access>read-only</access>
                <status>current</status>
                <description>The UDP port number on which the agent listens for&lt;br&gt;DPI packets.&lt;br&gt;If the value is zero, it means that the agent is&lt;br&gt;not listening for UDP DPI packets.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>dpiPathNameForUnixStream</objectName>
                <objectId>3</objectId>
                <parent>dpiPort</parent>
                <syntax>
                    <type>DisplayString</type>
                </syntax>
                <access>read-only</access>
                <status>current</status>
                <description>The full path name for a connection via an&lt;br&gt;AF_UNIX stream connection. The empty value&lt;br&gt;means the agent has no DPI AF_UNIX support.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectIdentifier>
                <objectName>dpiMIBConformance</objectName>
                <objectId>2</objectId>
                <parent>dpi20MIB</parent>
            </objectIdentifier>
        </mibObject>
        <mibObject>
            <objectIdentifier>
                <objectName>dpiMIBCompliances</objectName>
                <objectId>1</objectId>
                <parent>dpiMIBConformance</parent>
            </objectIdentifier>
        </mibObject>
        <mibObject>
            <objectIdentifier>
                <objectName>dpiMIBGroups</objectName>
                <objectId>2</objectId>
                <parent>dpiMIBConformance</parent>
            </objectIdentifier>
        </mibObject>
        <mibObject>
            <moduleCompliance>
                <objectName>dpiMIBCompliance</objectName>
                <objectId>1</objectId>
                <parent>dpiMIBCompliances</parent>
                <status>current</status>
                <description>The compliance statement for SNMPv2 entities&lt;br&gt;which implement the dpiMIB (DPI MIB).</description>
            </moduleCompliance>
        </mibObject>
        <mibObject>
            <moduleCompliance>
                <objectName>dpiMIBComplianceUnix</objectName>
                <objectId>2</objectId>
                <parent>dpiMIBCompliances</parent>
                <status>current</status>
                <description>The compliance statement for SNMPv2 entities&lt;br&gt;which implement the dpiMIB (DPI MIB), including&lt;br&gt;the new dpiGroupUnix group.</description>
            </moduleCompliance>
        </mibObject>
        <mibObject>
            <objectGroup>
                <objectName>dpiGroup</objectName>
                <objectId>1</objectId>
                <parent>dpiMIBGroups</parent>
                <groupMembers>
                    <member>dpiPortForTCP</member>
                    <member>dpiPortForUDP</member>
                </groupMembers>
                <status>current</status>
                <description>The dpiGroup defines the objects that are&lt;br&gt;used to figure out the DPI ports supported by&lt;br&gt;a DPI 2.0 capable SNMP agent.</description>
            </objectGroup>
        </mibObject>
        <mibObject>
            <objectGroup>
                <objectName>dpiGroupUnix</objectName>
                <objectId>2</objectId>
                <parent>dpiMIBGroups</parent>
                <groupMembers>
                    <member>dpiPathNameForUnixStream</member>
                </groupMembers>
                <status>current</status>
                <description>The dpiGroup defines the objects that are&lt;br&gt;used to figure out the DPI ports supported by&lt;br&gt;a DPI 2.0 capable SNMP agent.</description>
            </objectGroup>
        </mibObject>
    </objects>
</mib>
