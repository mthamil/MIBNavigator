<?xml version="1.0" encoding="UTF-8"?>
<mib definition="APPLICATION-MIB">
    <imports>
        <mibImport>
            <sourceMib>RFC1213-MIB</sourceMib>
            <import>DisplayString</import>
        </mibImport>
        <mibImport>
            <sourceMib>RFC-1212</sourceMib>
            <import>OBJECT-TYPE</import>
        </mibImport>
        <mibImport>
            <sourceMib>RFC1155-SMI</sourceMib>
            <import>Gauge</import>
            <import>Counter</import>
            <import>TimeTicks</import>
        </mibImport>
        <mibImport>
            <sourceMib>RFC1213-MIB</sourceMib>
            <import>mib-2</import>
        </mibImport>
    </imports>
    <objects>
        <mibObject>
            <objectIdentifier>
                <objectName>application</objectName>
                <objectId>27</objectId>
                <parent>mib-2</parent>
            </objectIdentifier>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>applTable</objectName>
                <objectId>1</objectId>
                <parent>application</parent>
                <syntax>
                    <sequence>ApplEntry</sequence>
                </syntax>
                <access>not-accessible</access>
                <status>mandatory</status>
                <description>The table holding objects which apply to all different&lt;br&gt;kinds of applications providing network services.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>applEntry</objectName>
                <objectId>1</objectId>
                <parent>applTable</parent>
                <syntax>
                    <type>ApplEntry</type>
                </syntax>
                <access>not-accessible</access>
                <status>mandatory</status>
                <description>An entry associated with a network service application.</description>
                <indices>
                    <index>applIndex</index>
                </indices>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>applIndex</objectName>
                <objectId>1</objectId>
                <parent>applEntry</parent>
                <syntax>
                    <type>INTEGER</type>
                </syntax>
                <access>not-accessible</access>
                <status>mandatory</status>
                <description>An index to uniquely identify the network service&lt;br&gt;application.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>applName</objectName>
                <objectId>2</objectId>
                <parent>applEntry</parent>
                <syntax>
                    <type>DisplayString</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>The name the network service application chooses to be&lt;br&gt;known by.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>applDirectoryName</objectName>
                <objectId>3</objectId>
                <parent>applEntry</parent>
                <syntax>
                    <type>DistinguishedName</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>The Distinguished Name of the directory entry where&lt;br&gt;static information about this application is stored.&lt;br&gt;An empty string indicates that no information about&lt;br&gt;the application is available in the directory.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>applVersion</objectName>
                <objectId>4</objectId>
                <parent>applEntry</parent>
                <syntax>
                    <type>DisplayString</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>The version of network service application software.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>applUptime</objectName>
                <objectId>5</objectId>
                <parent>applEntry</parent>
                <syntax>
                    <type>TimeStamp</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>The value of sysUpTime at the time the network service&lt;br&gt;application was last initialized.  If the application was&lt;br&gt;last initialized prior to the last initialization of the&lt;br&gt;network management subsystem, then this object contains&lt;br&gt;a zero value.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>applOperStatus</objectName>
                <objectId>6</objectId>
                <parent>applEntry</parent>
                <syntax>
                    <type>INTEGER</type>
                    <valueList>
                        <valueItem>
                            <label>up</label>
                            <val>1</val>
                        </valueItem>
                        <valueItem>
                            <label>down</label>
                            <val>2</val>
                        </valueItem>
                        <valueItem>
                            <label>halted</label>
                            <val>3</val>
                        </valueItem>
                        <valueItem>
                            <label>congested</label>
                            <val>4</val>
                        </valueItem>
                        <valueItem>
                            <label>restarting</label>
                            <val>5</val>
                        </valueItem>
                    </valueList>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>Indicates the operational status of the network service&lt;br&gt;application. 'down' indicates that the network service is&lt;br&gt;not available. 'running' indicates that the network service&lt;br&gt;is operational and available.  'halted' indicates that the&lt;br&gt;service is operational but not available.  'congested'&lt;br&gt;indicates that the service is operational but no additional&lt;br&gt;inbound associations can be accomodated.  'restarting'&lt;br&gt;indicates that the service is currently unavailable but is&lt;br&gt;in the process of restarting and will be available soon.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>applLastChange</objectName>
                <objectId>7</objectId>
                <parent>applEntry</parent>
                <syntax>
                    <type>TimeStamp</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>The value of sysUpTime at the time the network service&lt;br&gt;application entered its current operational state.  If&lt;br&gt;the current state was entered prior to the last&lt;br&gt;initialization of the local network management subsystem,&lt;br&gt;then this object contains a zero value.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>applInboundAssociations</objectName>
                <objectId>8</objectId>
                <parent>applEntry</parent>
                <syntax>
                    <type>Gauge</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>The number of current associations to the network service&lt;br&gt;application, where it is the responder.  For dynamic single&lt;br&gt;threaded processes, this will be the number of application&lt;br&gt;instances.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>applOutboundAssociations</objectName>
                <objectId>9</objectId>
                <parent>applEntry</parent>
                <syntax>
                    <type>Gauge</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>The number of current associations to the network service&lt;br&gt;application, where it is the initiator.  For dynamic single&lt;br&gt;threaded processes, this will be the number of application&lt;br&gt;instances.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>applAccumulatedInboundAssociations</objectName>
                <objectId>10</objectId>
                <parent>applEntry</parent>
                <syntax>
                    <type>Counter</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>The total number of associations to the application entity&lt;br&gt;since application initialization, where it was the responder.&lt;br&gt;For  dynamic single threaded processes, this will be the&lt;br&gt;number of application instances.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>applAccumulatedOutboundAssociations</objectName>
                <objectId>11</objectId>
                <parent>applEntry</parent>
                <syntax>
                    <type>Counter</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>The total number of associations to the application entity&lt;br&gt;since application initialization, where it was the initiator.&lt;br&gt;For dynamic single threaded processes, this will be the&lt;br&gt;number of application instances.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>applLastInboundActivity</objectName>
                <objectId>12</objectId>
                <parent>applEntry</parent>
                <syntax>
                    <type>TimeStamp</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>The value of sysUpTime at the time this application last&lt;br&gt;had an inbound association.  If the last association&lt;br&gt;occurred prior to the last initialization of the network&lt;br&gt;subsystem, then this object contains a zero value.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>applLastOutboundActivity</objectName>
                <objectId>13</objectId>
                <parent>applEntry</parent>
                <syntax>
                    <type>TimeStamp</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>The value of sysUpTime at the time this application last&lt;br&gt;had an outbound association.  If the last association&lt;br&gt;occurred prior to the last initialization of the network&lt;br&gt;subsystem, then this object contains a zero value.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>applRejectedInboundAssociations</objectName>
                <objectId>14</objectId>
                <parent>applEntry</parent>
                <syntax>
                    <type>Counter</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>The total number of inbound associations the application&lt;br&gt;entity has rejected, since application initialization.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>applFailedOutboundAssociations</objectName>
                <objectId>15</objectId>
                <parent>applEntry</parent>
                <syntax>
                    <type>Counter</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>The total number associations where the application entity&lt;br&gt;is initiator and association establishment has failed,&lt;br&gt;since application initialization.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>assocTable</objectName>
                <objectId>2</objectId>
                <parent>application</parent>
                <syntax>
                    <sequence>AssocEntry</sequence>
                </syntax>
                <access>not-accessible</access>
                <status>mandatory</status>
                <description>The table holding a set of all active application&lt;br&gt;associations.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>assocEntry</objectName>
                <objectId>1</objectId>
                <parent>assocTable</parent>
                <syntax>
                    <type>AssocEntry</type>
                </syntax>
                <access>not-accessible</access>
                <status>mandatory</status>
                <description>An entry associated with an association for a network&lt;br&gt;service application.</description>
                <indices>
                    <index>applIndex</index>
                    <index>assocIndex</index>
                </indices>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>assocIndex</objectName>
                <objectId>1</objectId>
                <parent>assocEntry</parent>
                <syntax>
                    <type>INTEGER</type>
                </syntax>
                <access>not-accessible</access>
                <status>mandatory</status>
                <description>An index to uniquely identify each association for a network&lt;br&gt;service application.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>assocRemoteApplication</objectName>
                <objectId>2</objectId>
                <parent>assocEntry</parent>
                <syntax>
                    <type>DisplayString</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>The name of the system running remote network service&lt;br&gt;application.  For an IP-based application this should be&lt;br&gt;either a domain name or IP address.  For an OSI application&lt;br&gt;it should be the string encoded distinguished name of the&lt;br&gt;managed object.  For X.400(84) MTAs which do not have a&lt;br&gt;Distinguished Name, the RFC1327 [6] syntax&lt;br&gt;'mta in globalid' should be used.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>assocApplicationProtocol</objectName>
                <objectId>3</objectId>
                <parent>assocEntry</parent>
                <syntax>
                    <type>OBJECT IDENTIFIER</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>An identification of the protocol being used for the&lt;br&gt;application.  For an OSI Application, this will be the&lt;br&gt;Application Context.  For Internet applications, the IANA&lt;br&gt;maintains a registry of the OIDs which correspond to&lt;br&gt;well-known applications.  If the application protocol is&lt;br&gt;not listed in the registry, an OID value of the form&lt;br&gt;{applTCPProtoID port} or {applUDProtoID port} are used for&lt;br&gt;TCP-based and UDP-based protocols, respectively. In either&lt;br&gt;case 'port' corresponds to the primary port number being&lt;br&gt;used by the protocol.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>assocApplicationType</objectName>
                <objectId>4</objectId>
                <parent>assocEntry</parent>
                <syntax>
                    <type>INTEGER</type>
                    <valueList>
                        <valueItem>
                            <label>ua-initiator</label>
                            <val>1</val>
                        </valueItem>
                        <valueItem>
                            <label>ua-responder</label>
                            <val>2</val>
                        </valueItem>
                        <valueItem>
                            <label>peer-initiator</label>
                            <val>3</val>
                        </valueItem>
                        <valueItem>
                            <label>peer-responder</label>
                            <val>4</val>
                        </valueItem>
                    </valueList>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>This indicates whether the remote application is some type of&lt;br&gt;client making use of this network service (e.g. a User Agent)&lt;br&gt;or a server acting as a peer. Also indicated is whether the&lt;br&gt;remote end initiated an incoming connection to the network&lt;br&gt;service or responded to an outgoing connection made by the&lt;br&gt;local application.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>assocDuration</objectName>
                <objectId>5</objectId>
                <parent>assocEntry</parent>
                <syntax>
                    <type>TimeStamp</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>The value of sysUpTime at the time this association was&lt;br&gt;started.  If this association started prior to the last&lt;br&gt;initialization of the network subsystem, then this&lt;br&gt;object contains a zero value.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectIdentifier>
                <objectName>applConformance</objectName>
                <objectId>3</objectId>
                <parent>application</parent>
            </objectIdentifier>
        </mibObject>
        <mibObject>
            <objectIdentifier>
                <objectName>applGroups</objectName>
                <objectId>1</objectId>
                <parent>applConformance</parent>
            </objectIdentifier>
        </mibObject>
        <mibObject>
            <objectIdentifier>
                <objectName>applCompliances</objectName>
                <objectId>2</objectId>
                <parent>applConformance</parent>
            </objectIdentifier>
        </mibObject>
        <mibObject>
            <objectIdentifier>
                <objectName>applCompliance</objectName>
                <objectId>1</objectId>
                <parent>applCompliances</parent>
            </objectIdentifier>
        </mibObject>
        <mibObject>
            <objectIdentifier>
                <objectName>assocCompliance</objectName>
                <objectId>2</objectId>
                <parent>applCompliances</parent>
            </objectIdentifier>
        </mibObject>
        <mibObject>
            <objectIdentifier>
                <objectName>applGroup</objectName>
                <objectId>1</objectId>
                <parent>applGroups</parent>
            </objectIdentifier>
        </mibObject>
        <mibObject>
            <objectIdentifier>
                <objectName>assocGroup</objectName>
                <objectId>2</objectId>
                <parent>applGroups</parent>
            </objectIdentifier>
        </mibObject>
        <mibObject>
            <objectIdentifier>
                <objectName>applTCPProtoID</objectName>
                <objectId>4</objectId>
                <parent>application</parent>
            </objectIdentifier>
        </mibObject>
        <mibObject>
            <objectIdentifier>
                <objectName>applUDPProtoID</objectName>
                <objectId>5</objectId>
                <parent>application</parent>
            </objectIdentifier>
        </mibObject>
    </objects>
</mib>
