        <mibObject>
            <objectType>
                <objectName>mtaGroupFailedRecipients</objectName>
                <objectId>13</objectId>
                <parent>extendedMtaGroupEntry</parent>
                <syntax>
                    <type>Counter</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>The total number of recipients included in messages that have 'failed'&lt;br&gt;group processing since this group was initialized.</description>
            </objectType>
        </mibObject>
