        </mibObject>
        <mibObject>
            <objectType>
                <objectName>extendedApplTable</objectName>
                <objectId>1</objectId>
                <parent>extendedMADMAN</parent>
                <syntax>
                    <sequence>ExtendedApplEntry</sequence>
                </syntax>
                <access>not-accessible</access>
                <status>mandatory</status>
                <description>The table holding information specific to an Application for which&lt;br&gt;extended information is available.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>extendedApplEntry</objectName>
                <objectId>1</objectId>
                <parent>extendedApplTable</parent>
                <syntax>
                    <type>ExtendedApplEntry</type>
                </syntax>
                <access>not-accessible</access>
                <status>mandatory</status>
                <description>The entry associated with each application for which extended information&lt;br&gt;is available.</description>
                <indices>
                    <index>applIndex</index>
                </indices>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>applDescr</objectName>
                <objectId>1</objectId>
                <parent>extendedApplEntry</parent>
                <syntax>
                    <type>DisplayString</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>A textual description for the application. This should identify the type of&lt;br&gt;application as well as any other pertinent details regarding its implementation.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>applContact</objectName>
                <objectId>2</objectId>
                <parent>extendedApplEntry</parent>
                <syntax>
                    <type>DisplayString</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>The textual identification of the contact person for this application, together&lt;br&gt;with information on how to contact that person..</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>applDesiredOperStatus</objectName>
                <objectId>3</objectId>
                <parent>extendedApplEntry</parent>
                <syntax>
                    <type>INTEGER</type>
                    <valueList>
                        <valueItem>
                            <label>up</label>
                            <val>1</val>
                        </valueItem>
                        <valueItem>
                            <label>down</label>
                            <val>2</val>
                        </valueItem>
                        <valueItem>
                            <label>halted</label>
                            <val>3</val>
                        </valueItem>
                    </valueList>
                </syntax>
                <access>read-write</access>
                <status>mandatory</status>
                <description>Indicates the intended operational state of the application&lt;br&gt;up  	- application is operational and available&lt;br&gt;down 	- application is not operational&lt;br&gt;halted 	- application is operational, but not available.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>extendedMtaTable</objectName>
                <objectId>2</objectId>
                <parent>extendedMADMAN</parent>
                <syntax>
                    <sequence>ExtendedMtaEntry</sequence>
                </syntax>
                <access>not-accessible</access>
                <status>mandatory</status>
                <description>The table holding information specific to an MTA for which&lt;br&gt;extended information is available.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>extendedMtaEntry</objectName>
                <objectId>1</objectId>
                <parent>extendedMtaTable</parent>
