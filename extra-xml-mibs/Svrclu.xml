        </mibObject>
        <mibObject>
            <objectType>
                <objectName>svrCluMibMajorRev</objectName>
                <objectId>1</objectId>
                <parent>svrCluMibInfo</parent>
                <syntax>
                    <type>INTEGER</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>Major revision of this MIB. Should equal 2.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>svrCluMibMinorRev</objectName>
                <objectId>2</objectId>
                <parent>svrCluMibInfo</parent>
                <syntax>
                    <type>INTEGER</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>Minor Revision of the MIB. Should equal 0.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>svrCluSoftwareVendor</objectName>
                <objectId>1</objectId>
                <parent>svrCluClusterInfo</parent>
                <syntax>
                    <type>DisplayString</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>Cluster software vendor name.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>svrCluSoftwareVersion</objectName>
                <objectId>2</objectId>
                <parent>svrCluClusterInfo</parent>
                <syntax>
                    <type>DisplayString</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>Cluster software version.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>svrCluSoftwareStatus</objectName>
                <objectId>3</objectId>
                <parent>svrCluClusterInfo</parent>
                <syntax>
                    <type>ClusterStatus</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>Status of the cluster software: installed, running, failed, etc.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>svrCluClusterType</objectName>
                <objectId>4</objectId>
                <parent>svrCluClusterInfo</parent>
                <syntax>
                    <type>ClusterType</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>Type of cluster which is running. Must use formula defined at&lt;br&gt;ClusterType enumeration list.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>svrCluExtensionOID</objectName>
                <objectId>5</objectId>
                <parent>svrCluClusterInfo</parent>
                <syntax>
                    <type>OBJECT IDENTIFIER</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>The authoritative identification for the MIB that further&lt;br&gt;instruments this specific cluster technology.  If no such&lt;br&gt;identifier exists, the value {0.0} should be returned.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>svrCluThisMember</objectName>
                <objectId>6</objectId>
                <parent>svrCluClusterInfo</parent>
                <syntax>
                    <type>INTEGER</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>Index into the member table (svrCluMemberTable) which corresponds to this node.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>svrCluClusterName</objectName>
                <objectId>7</objectId>
                <parent>svrCluClusterInfo</parent>
                <syntax>
                    <type>DisplayString</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>The name/alias for this cluster.  This must be a unique name not&lt;br&gt;equal to any of the member names or any network name</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>svrCluClusterAddressTable</objectName>
                <objectId>8</objectId>
                <parent>svrCluClusterInfo</parent>
                <syntax>
                    <sequence>SvrCluClusterAddressEntry</sequence>
                </syntax>
                <access>not-accessible</access>
                <status>mandatory</status>
                <description>This table represents the cluster (aliases) IP&lt;br&gt;addresses.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>svrCluClusterAddressEntry</objectName>
                <objectId>1</objectId>
                <parent>svrCluClusterAddressTable</parent>
                <syntax>
                    <type>SvrCluClusterAddressEntry</type>
                </syntax>
                <access>not-accessible</access>
                <status>mandatory</status>
                <description>Cluster alias IP addresses</description>
                <indices>
                    <index>svrCluClusterAddressIndex</index>
                </indices>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>svrCluClusterAddressIndex</objectName>
                <objectId>1</objectId>
                <parent>svrCluClusterAddressEntry</parent>
                <syntax>
                    <type>INTEGER</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>The index for this address.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>svrCluClusterAddress</objectName>
                <objectId>2</objectId>
                <parent>svrCluClusterAddressEntry</parent>
                <syntax>
                    <type>IpAddress</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>An IP address of this cluster alias.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>svrCluServiceName</objectName>
                <objectId>9</objectId>
                <parent>svrCluClusterInfo</parent>
                <syntax>
                    <type>DisplayString</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>Returns the service name of the cluster.&lt;br&gt;Examples are MSCS, NCS, NHAS.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>svrCluMemberTable</objectName>
                <objectId>11</objectId>
                <parent>svrCluClusterInfo</parent>
                <syntax>
                    <sequence>SvrCluMemberEntry</sequence>
                </syntax>
                <access>not-accessible</access>
                <status>mandatory</status>
                <description>Table of cluster members.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>svrCluMemberEntry</objectName>
                <objectId>1</objectId>
                <parent>svrCluMemberTable</parent>
                <syntax>
                    <type>SvrCluMemberEntry</type>
                </syntax>
                <access>not-accessible</access>
                <status>mandatory</status>
                <description>An entry pertaining to one particular member node.</description>
                <indices>
                    <index>svrCluMemberIndex</index>
                </indices>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>svrCluMemberIndex</objectName>
                <objectId>1</objectId>
                <parent>svrCluMemberEntry</parent>
                <syntax>
                    <type>INTEGER</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>A unique index for the entry.  Values of svrCluMemberIndex must&lt;br&gt;remain constant at least between reboots of the network management&lt;br&gt;system on the managed node.  Where possible, this value should&lt;br&gt;reflect the system's native member identifier.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>svrCluMemberName</objectName>
                <objectId>2</objectId>
                <parent>svrCluMemberEntry</parent>
                <syntax>
                    <type>DisplayString</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>The host name of this cluster member.  A zero-length value means&lt;br&gt;the member's host name is unknown.  This name may not necessarily&lt;br&gt;resolve to an address.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>svrCluMemberComment</objectName>
                <objectId>3</objectId>
                <parent>svrCluMemberEntry</parent>
                <syntax>
                    <type>DisplayString</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>Description of the node.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>svrCluMemberStatus</objectName>
                <objectId>4</objectId>
                <parent>svrCluMemberEntry</parent>
                <syntax>
                    <type>MemberStatus</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>Status of this member: new, normal, removed, etc.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>svrCluMemberAddressTable</objectName>
                <objectId>12</objectId>
                <parent>svrCluClusterInfo</parent>
                <syntax>
                    <sequence>SvrCluMemberAddressEntry</sequence>
                </syntax>
                <access>not-accessible</access>
                <status>mandatory</status>
                <description>This table represents the managed nodes's knowledge of the&lt;br&gt;IP addreses configured on the other cluster members.&lt;br&gt;This provides a hint for reaching the other members, and&lt;br&gt;a low-level mechanism for identifying members of the same&lt;br&gt;cluster.&lt;br&gt;&lt;br&gt;Note that the managed node's addressing information is already&lt;br&gt;available in ipAddrTable, so is not required to be present in&lt;br&gt;this table</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>svrCluMemberAddressEntry</objectName>
                <objectId>1</objectId>
                <parent>svrCluMemberAddressTable</parent>
                <syntax>
                    <type>SvrCluMemberAddressEntry</type>
                </syntax>
                <access>not-accessible</access>
                <status>mandatory</status>
                <description>A particular cluster member IP address</description>
                <indices>
                    <index>svrCluMemberIndex</index>
                    <index>svrCluMemberAddressIndex</index>
                </indices>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>svrCluMemberAddressIndex</objectName>
                <objectId>1</objectId>
                <parent>svrCluMemberAddressEntry</parent>
                <syntax>
                    <type>INTEGER</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>The index for this address.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>svrCluMemberAddress</objectName>
                <objectId>2</objectId>
                <parent>svrCluMemberAddressEntry</parent>
                <syntax>
                    <type>IpAddress</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>An IP address of this cluster member. This address may not be&lt;br&gt;reachable from nodes that are not configured into the cluster.</description>
            </objectType>
        </mibObject>
    </objects>
</mib>
