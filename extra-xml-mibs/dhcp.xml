            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>parDhcpTotalNoOfAcks</objectName>
                <objectId>6</objectId>
                <parent>dhcpPar</parent>
                <syntax>
                    <type>Counter</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
