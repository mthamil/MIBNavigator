        </mibObject>
        <mibObject>
            <objectType>
                <objectName>parDhcpTotalNoOfRequests</objectName>
                <objectId>3</objectId>
                <parent>dhcpPar</parent>
                <syntax>
                    <type>Counter</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
