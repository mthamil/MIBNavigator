                <objectId>3</objectId>
                <parent>ipxIfEntry</parent>
                <syntax>
                    <type>INTEGER</type>
                    <valueList>
                        <valueItem>
                            <label>down</label>
                            <val>1</val>
                        </valueItem>
                        <valueItem>
                            <label>up</label>
                            <val>2</val>
                        </valueItem>
                        <valueItem>
                            <label>sleeping</label>
                            <val>3</val>
                        </valueItem>
                    </valueList>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
