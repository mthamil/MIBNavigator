            <objectType>
                <objectName>s5EnSStatSegRate</objectName>
                <objectId>3</objectId>
                <parent>s5EnSStatEntry</parent>
                <syntax>
                    <type>INTEGER</type>
                    <nameValuePairs>
                        <nameValuePair>
                            <pairName>unknown</pairName>
                            <pairValue>1</pairValue>
                        </nameValuePair>
                        <nameValuePair>
                            <pairName>bps10m</pairName>
                            <pairValue>2</pairValue>
                        </nameValuePair>
                        <nameValuePair>
                            <pairName>bps100m</pairName>
