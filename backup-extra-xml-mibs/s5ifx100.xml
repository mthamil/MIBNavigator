<?xml version="1.0" encoding="UTF-8"?>
<mib definition="BN-IF-EXTENSIONS-MIB">
    <imports>
        <mibImport>
            <sourceMib>RFC-1212</sourceMib>
            <import>OBJECT-TYPE</import>
        </mibImport>
        <mibImport>
            <sourceMib>S5-ROOT-MIB</sourceMib>
            <import>s5IfExt</import>
        </mibImport>
    </imports>
    <objects>
        <mibObject>
            <objectIdentifier>
                <objectName>bnIfExtensions</objectName>
                <objectId>1</objectId>
                <parent>s5IfExt</parent>
            </objectIdentifier>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>bnIfExtnTable</objectName>
                <objectId>1</objectId>
                <parent>bnIfExtensions</parent>
                <syntax>
                    <sequence>BnIfExtnEntry</sequence>
                </syntax>
                <access>not-accessible</access>
                <status>mandatory</status>
                <description>A list of interface entries. The numebr of entries is&lt;br&gt;given by the value of ifNumber.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>bnIfExtnEntry</objectName>
                <objectId>1</objectId>
                <parent>bnIfExtnTable</parent>
                <syntax>
                    <type>BnIfExtnEntry</type>
                </syntax>
                <access>not-accessible</access>
                <status>mandatory</status>
                <description>An interface entry containing objects at the subnetwork&lt;br&gt;layer and below for a particular interface.</description>
                <indices>
                    <index>bnIfExtnIndex</index>
                </indices>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>bnIfExtnIndex</objectName>
                <objectId>1</objectId>
                <parent>bnIfExtnEntry</parent>
                <syntax>
                    <type>INTEGER</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>A unique value for each interface corresponding to the&lt;br&gt;ifIndex value for the same interface.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>bnIfExtnSlot</objectName>
                <objectId>2</objectId>
                <parent>bnIfExtnEntry</parent>
                <syntax>
                    <type>INTEGER</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>The unit number associated with this particular interface.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>bnIfExtnPort</objectName>
                <objectId>3</objectId>
                <parent>bnIfExtnEntry</parent>
                <syntax>
                    <type>INTEGER</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>The port number within a unit or slot.</description>
            </objectType>
        </mibObject>
    </objects>
</mib>
