                <objectName>dot3ChipSetAMD7990</objectName>
                <objectId>1</objectId>
                <parent>dot3ChipSetAMD</parent>
            </objectIdentifier>
        </mibObject>
        <mibObject>
            <objectIdentifier>
                <objectName>dot3ChipSetAMD79900</objectName>
                <objectId>2</objectId>
                <parent>dot3ChipSetAMD</parent>
            </objectIdentifier>
        </mibObject>
        <mibObject>
            <objectIdentifier>
                <objectName>dot3ChipSetIntel</objectName>
                <objectId>2</objectId>
                <parent>dot3ChipSets</parent>
            </objectIdentifier>
        </mibObject>
        <mibObject>
            <objectIdentifier>
                <objectName>dot3ChipSetIntel82586</objectName>
                <objectId>1</objectId>
                <parent>dot3ChipSetIntel</parent>
            </objectIdentifier>
        </mibObject>
        <mibObject>
            <objectIdentifier>
                <objectName>dot3ChipSetIntel82596</objectName>
                <objectId>2</objectId>
                <parent>dot3ChipSetIntel</parent>
            </objectIdentifier>
        </mibObject>
        <mibObject>
            <objectIdentifier>
                <objectName>dot3ChipSetSeeq</objectName>
                <objectId>3</objectId>
                <parent>dot3ChipSets</parent>
            </objectIdentifier>
        </mibObject>
        <mibObject>
            <objectIdentifier>
                <objectName>dot3ChipSetSeeq8003</objectName>
                <objectId>1</objectId>
