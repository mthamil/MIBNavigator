                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>The number of data packets and control packets sent to the host&lt;br&gt;by this station.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>esconStationDataPacketsRetransmitted</objectName>
                <objectId>15</objectId>
                <parent>esconStationEntry</parent>
                <syntax>
                    <type>Counter32</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>The number of data packets retransmitted by this station</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>esconStationPositiveAckDataPackets</objectName>
                <objectId>16</objectId>
                <parent>esconStationEntry</parent>
                <syntax>
                    <type>Counter32</type>
                </syntax>
