        </mibObject>
        <mibObject>
            <objectType>
                <objectName>esconStationHostLinkAddress</objectName>
                <objectId>1</objectId>
                <parent>esconStationEntry</parent>
                <syntax>
                    <type>OCTET STRING(SIZE(1))</type>
                </syntax>
                <access>not-accessible</access>
                <status>mandatory</status>
                <description>This address identifies the ESCON Director port to which the&lt;br&gt;optical fiber between the ESCON Director and the host is&lt;br&gt;attached.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>esconStationPartitionNumber</objectName>
                <objectId>2</objectId>
                <parent>esconStationEntry</parent>
                <syntax>
                    <type>OCTET STRING(SIZE(1))</type>
                </syntax>
                <access>not-accessible</access>
                <status>mandatory</status>
                <description>A number identifying a logical host within an actual host.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>esconStationDeviceAddress</objectName>
                <objectId>3</objectId>
