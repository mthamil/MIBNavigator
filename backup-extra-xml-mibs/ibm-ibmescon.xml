            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>esconLinkStatus</objectName>
                <objectId>3</objectId>
                <parent>esconLinkEntry</parent>
                <syntax>
                    <type>INTEGER</type>
                    <valueList>
                        <valueItem>
                            <label>hlpNotEstab</label>
                            <val>1</val>
                        </valueItem>
                        <valueItem>
                            <label>hlpEstab</label>
                            <val>2</val>
                        </valueItem>
                        <valueItem>
                            <label>hlpError</label>
                            <val>3</val>
