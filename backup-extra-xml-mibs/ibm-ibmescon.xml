        </mibObject>
        <mibObject>
            <objectType>
                <objectName>esconStationDataPacketsKoReceived</objectName>
                <objectId>12</objectId>
                <parent>esconStationEntry</parent>
                <syntax>
                    <type>Counter32</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>The number of data packets received from the host by this station&lt;br&gt;with Data Check.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
