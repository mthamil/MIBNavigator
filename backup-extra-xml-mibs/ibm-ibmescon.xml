                <syntax>
                    <type>INTEGER(1..65535)</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>Number of buffers in the host buffer pool for receiving data&lt;br&gt;from this station.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>esconStationUnitSize</objectName>
                <objectId>8</objectId>
                <parent>esconStationEntry</parent>
                <syntax>
                    <type>INTEGER(64..4000)</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>Maximum size of a buffer, in bytes, that the host can receive&lt;br&gt;from this station.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>esconStationMaxMsgSizeReceived</objectName>
                <objectId>9</objectId>
                <parent>esconStationEntry</parent>
                <syntax>
                    <type>INTEGER(0..65535)</type>
                </syntax>
                <access>read-write</access>
