            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>esconStationSecondChanceAttentions</objectName>
                <objectId>17</objectId>
                <parent>esconStationEntry</parent>
                <syntax>
                    <type>Counter32</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>The number of times this station has sent a Second Chance&lt;br&gt;Attention signal to the host.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>esconStationCommandsRetried</objectName>
                <objectId>18</objectId>
                <parent>esconStationEntry</parent>
                <syntax>
                    <type>Counter32</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>The number of times a command has been retried on this&lt;br&gt;station</description>
            </objectType>
        </mibObject>
        <mibObject>
