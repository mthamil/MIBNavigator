        <mibObject>
            <objectType>
                <objectName>ibmtn3270eConnRejectTable</objectName>
                <objectId>1</objectId>
                <parent>ibmtn3270eMIB</parent>
                <syntax>
                    <sequence>Ibmtn3270eConnRejectEntry</sequence>
                </syntax>
                <access>not-accessible</access>
                <status>mandatory</status>
                <description>A table of information about TN3270E connection rejections.&lt;br&gt;This table does not contain information for connections that&lt;br&gt;were successfully established, but terminated abnormally.</description>
            </objectType>
        </mibObject>
        <mibObject>
