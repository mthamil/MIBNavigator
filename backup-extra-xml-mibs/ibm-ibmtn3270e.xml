                            <val>0</val>
                        </valueItem>
                        <valueItem>
                            <label>ipv4</label>
                            <val>1</val>
                        </valueItem>
                        <valueItem>
                            <label>ipv6</label>
                            <val>2</val>
                        </valueItem>
                    </valueList>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>A indication of the type of client address provided in&lt;br&gt;ibmtn3270eConnRejectClient.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>ibmtn3270eConnRejectClient</objectName>
                <objectId>3</objectId>
                <parent>ibmtn3270eConnRejectEntry</parent>
                <syntax>
                    <type>OCTET STRING(SIZE(0..255))</type>
                </syntax>
                <access>read-only</access>
                <status>mandatory</status>
                <description>The IP address of the client whose connection was rejected.</description>
            </objectType>
        </mibObject>
        <mibObject>
            <objectType>
                <objectName>ibmtn3270eConnRejectReason</objectName>
