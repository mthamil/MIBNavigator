                            <val>15</val>
                        </valueItem>
                        <valueItem>
                            <label>termtypagain</label>
                            <val>16</val>
                        </valueItem>
                        <valueItem>
                            <label>noportagain</label>
                            <val>17</val>
                        </valueItem>
                        <valueItem>
                            <label>prtnoluname</label>
                            <val>18</val>
                        </valueItem>
                        <valueItem>
                            <label>clinoauthent</label>
                            <val>19</val>
                        </valueItem>
                        <valueItem>
                            <label>clinoauthflt</label>
                            <val>20</val>
                        </valueItem>
                        <valueItem>
                            <label>noluconf</label>
                            <val>21</val>
                        </valueItem>
                        <valueItem>
                            <label>noportmore</label>
                            <val>22</val>
                        </valueItem>
                        <valueItem>
                            <label>noresource</label>
