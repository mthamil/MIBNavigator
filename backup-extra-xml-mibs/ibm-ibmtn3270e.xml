                <objectId>1</objectId>
                <parent>ibmtn3270eConnRejectTable</parent>
                <syntax>
                    <type>Ibmtn3270eConnRejectEntry</type>
                </syntax>
                <access>not-accessible</access>
                <status>mandatory</status>
                <description>Information indicating why a particular client connection&lt;br&gt;was rejected.</description>
